type Query {
    customerById(id: ID): Customer
    customers : [Customer]
}

type Subscription {
    customers: String
    stockUpdates(name :String) : StockUpdate
}

type StockUpdate {
 price : Float
 name : String
}

type Customer {
    id: ID!
    name : String
    email : String
    profile: CustomerProfile
}

type CustomerProfile {
 id : ID!
}

